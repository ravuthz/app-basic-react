{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier/react"],
  "plugins": ["react", "jsx-a11y", "import"],

  "env": { "browser": true, "commonjs": true },

  "rules": {
    "no-console": 0,
    "linebreak-style": ["error", "unix"],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
